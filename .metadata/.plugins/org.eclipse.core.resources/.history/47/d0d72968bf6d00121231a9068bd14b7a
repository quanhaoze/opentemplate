#include <stdio.h>
#include <stdlib.h>
#include "bit.h"

int main(int argc, char *argv[])
{
	unsigned char p1[] = { 0x00,0x00,0x01};
	unsigned char p2[] = { 0x80,0x24,0x31};
	unsigned char p3[3]= {0x00,0x00,0x01};

	//bit_rot_left(p3,24,23);

	for (int i = 0; i < 3; i++)
		printf("%#x\n",(unsigned char)p3[i] << -1);

	return 0;

}
